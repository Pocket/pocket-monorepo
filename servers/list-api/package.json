{
  "main": "dist/server/main.js",
  "name": "list-api",
  "scripts": {
    "build": "rm -rf dist && tsc && npm run build-schema",
    "watch": "tsc -w & nodemon",
    "start": "node dist/main.js",
    "dev": "npm run build && npm run watch",
    "build-schema": "node dist/server/buildSchema.js",
    "test:watch": "npm test -- --watchAll",
    "test": "jest \"\\.spec\\.ts\"",
    "test-integrations": "jest \"\\.integration\\.ts\" --runInBand",
    "lint": "eslint --fix-dry-run \"src/**/*.ts\"",
    "format": "eslint --fix \"src/**/*.ts\""
  },
  "dependencies": {
    "@aws-sdk/client-eventbridge": "3.458.0",
    "@aws-sdk/client-kinesis": "3.458.0",
    "@aws-sdk/client-sqs": "3.458.0",
    "@pocket-tools/apollo-cursor-pagination": "1.0.3",
    "@pocket-tools/apollo-utils": "workspace:*",
    "@pocket-tools/backend-benchmarking": "1.1.0",
    "@pocket-tools/ts-logger": "workspace:*",
    "@sentry/node": "7.91.0",
    "@sentry/tracing": "7.91.0",
    "@snowplow/node-tracker": "3.19.0",
    "dataloader": "2.2.2",
    "express": "4.18.2",
    "express-validator": "^7.0.1",
    "graphql": "16.8.1",
    "graphql-constraint-directive": "5.3.0",
    "graphql-depth-limit": "1.1.0",
    "graphql-tag": "2.12.6",
    "knex": "3.1.0",
    "locutus": "2.0.16",
    "lodash": "4.17.21",
    "luxon": "3.4.4",
    "mysql2": "3.6.5",
    "unleash-client": "5.3.0"
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "tsconfig": "workspace:*",
    "@types/chai": "4.3.11",
    "@types/chai-as-promised": "7.1.8",
    "@types/chai-datetime": "0.0.39",
    "@types/chance": "1.1.6",
    "@types/deep-equal-in-any-order": "1.0.3",
    "@types/graphql-depth-limit": "1.1.6",
    "@types/graphql-upload": "16.0.5",
    "@types/jest": "29.5.11",
    "@types/locutus": "0.0.8",
    "@types/lodash": "4.14.202",
    "@types/luxon": "3.3.5",
    "@types/sinon": "17.0.2",
    "@types/supertest": "^2.0.16",
    "chai": "4.3.10",
    "chai-as-promised": "7.1.1",
    "chai-datetime": "1.8.0",
    "chai-shallow-deep-equal": "1.4.6",
    "chance": "1.1.11",
    "deep-equal-in-any-order": "2.0.6",
    "jest-extended": "4.0.2",
    "nock": "13.4.0",
    "nodemon": "3.0.2",
    "sinon": "17.0.1",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1"
  },
  "files": [
    "dist",
    "schema.graphql",
    "package.json"
  ]
