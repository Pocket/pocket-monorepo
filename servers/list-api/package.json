{
  "main": "dist/server/main.js",
  "name": "list-api",
  "scripts": {
    "build": "rm -rf dist && tsc && npm run build-schema",
    "watch": "tsc -w --preserveWatchOutput & nodemon --config ../../nodemon.json",
    "start": "node dist/server/main.js",
    "dev": "npm run build && npm run watch",
    "build-schema": "node dist/server/buildSchema.js",
    "test:watch": "npm test -- --watchAll",
    "test": "jest \"\\.spec\\.ts\"",
    "test-integrations": "jest \"\\.integration\\.ts\" --runInBand",
    "lint": "eslint --fix-dry-run \"src/**/*.ts\"",
    "format": "eslint --fix \"src/**/*.ts\""
  },
  "dependencies": {
    "@aws-sdk/client-eventbridge": "3.499.0",
    "@aws-sdk/client-kinesis": "3.499.0",
    "@aws-sdk/client-sqs": "3.499.0",
    "@pocket-tools/apollo-cursor-pagination": "1.0.3",
    "@pocket-tools/apollo-utils": "workspace:*",
    "@pocket-tools/tracing": "workspace:*",
    "@pocket-tools/ts-logger": "workspace:*",
    "@sentry/node": "7.98.0",
    "@sentry/tracing": "7.98.0",
    "@snowplow/node-tracker": "3.20.0",
    "dataloader": "2.2.2",
    "express": "4.18.2",
    "express-validator": "^7.0.1",
    "graphql": "16.8.1",
    "graphql-constraint-directive": "5.3.0",
    "graphql-depth-limit": "1.1.0",
    "graphql-tag": "2.12.6",
    "knex": "3.1.0",
    "locutus": "2.0.16",
    "lodash": "4.17.21",
    "luxon": "3.4.4",
    "mysql2": "3.6.5",
    "unleash-client": "5.3.0"
  },
  "devDependencies": {
    "@pocket-tools/backend-benchmarking": "workspace:*",
    "@types/chance": "1.1.6",
    "@types/graphql-depth-limit": "1.1.6",
    "@types/graphql-upload": "16.0.7",
    "@types/jest": "29.5.11",
    "@types/locutus": "0.0.8",
    "@types/lodash": "4.14.202",
    "@types/luxon": "3.4.2",
    "@types/supertest": "^6.0.2",
    "chance": "1.1.11",
    "eslint-config-custom": "workspace:*",
    "jest": "29.7.0",
    "jest-extended": "4.0.2",
    "nock": "13.5.0",
    "nodemon": "3.0.3",
    "supertest": "6.3.4",
    "ts-jest": "29.1.2",
    "tsconfig": "workspace:*"
  },
  "files": [
    "dist",
    "schema.graphql",
    "package.json"
  ]
}
