{
  "name": "shareable-lists-api",
  "main": "dist/main.js",
  "prisma": {
    "seed": "ts-node --emit=false prisma/seed.ts"
  },
  "scripts": {
    "build": "rm -rf dist && tsc && npm run build-schema-admin && npm run build-schema-public",
    "watch": "tsc -w & nodemon",
    "start": "npm run migrate:deploy && node dist/main.js",
    "start:dev": "npm run migrate:deploy && npm run build && npm run watch",
    "test-ci": "npm test",
    "test": "jest \"\\.spec\\.ts\"",
    "test-integrations": "jest \"\\.integration\\.ts\" --runInBand",
    "lint-check": "eslint --fix-dry-run \"src/**/*.ts\"",
    "lint-fix": "eslint --fix \"src/**/*.ts\"",
    "build-schema-admin": "node dist/admin/buildSchema.js",
    "build-schema-public": "node dist/public/buildSchema.js",
    "db:generate-client": "prisma generate",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:reset": "prisma migrate reset"
  },
  "dependencies": {
    "@apollo/server": "^4.7.4",
    "@apollo/server-plugin-landing-page-graphql-playground": "4.0.0",
    "@apollo/server-plugin-response-cache": "^4.1.0",
    "@apollo/subgraph": "^2.3.2",
    "@apollo/utils.keyvadapter": "^3.0.0",
    "@aws-sdk/client-eventbridge": "^3.370.0",
    "@keyv/redis": "^2.5.6",
    "@pocket-tools/apollo-utils": "3.4.2",
    "@pocket-tools/terraform-modules": "^4.16.2",
    "@pocket-tools/ts-logger": "^1.2.2",
    "@prisma/client": "^4.12.0",
    "@sentry/node": "7.61.1",
    "@sentry/serverless": "^7.49.0",
    "@sentry/tracing": "7.61.1",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "2.1430.0",
    "cors": "2.8.5",
    "express": "4.18.2",
    "express-validator": "^7.0.0",
    "fetch-retry": "^5.0.4",
    "graphql": "16.6.0",
    "graphql-constraint-directive": "^5.0.0",
    "graphql-tag": "2.12.6",
    "graphql-upload": "15.0.2",
    "isomorphic-fetch": "^3.0.0",
    "keyv": "^4.5.2",
    "sinon": "^15.0.1",
    "slugify": "1.6.6",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "8.0.2",
    "@pocket-tools/eslint-config": "2.0.0",
    "@pocket-tools/tsconfig": "^2.0.1",
    "@types/aws-lambda": "^8.10.114",
    "@types/chai": "4.3.5",
    "@types/jest": "29.5.3",
    "@types/node": "^18.11.19",
    "@types/node-fetch": "^2.6.3",
    "chai": "4.3.7",
    "dotenv": "16.3.1",
    "husky": "8.0.3",
    "jest": "29.6.2",
    "nock": "13.3.1",
    "nodemon": "2.0.22",
    "prisma": "^4.12.0",
    "supertest": "6.3.3",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.1.6"
  }
}
