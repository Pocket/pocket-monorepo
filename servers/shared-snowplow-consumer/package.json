    "@sentry/node": "7.101.1",
    "@sentry/tracing": "7.101.1",
    "@snowplow/node-tracker": "3.21.0",
    "express": "4.18.2",
    "express-validator": "^7.0.1",
    "node-fetch": "2.7.0"
  },
  "devDependencies": {
    "@faker-js/faker": "8.3.1",
