    "@opentelemetry/sdk-trace-node": "1.8.0",
    "@pocket-tools/apollo-utils": "3.4.0",
    "@pocket-tools/ts-logger": "workspace:*",
    "@sentry/node": "7.58.1",
    "@sentry/tracing": "7.58.1",
    "aws-sdk": "2.1138.0",
    "dataloader": "2.0.0",
    "express": "4.18.2",
    "graphql": "16.6.0",
    "knex": "0.95.15",
