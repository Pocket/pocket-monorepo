    "@opentelemetry/exporter-trace-otlp-http": "0.34.0",
    "@opentelemetry/id-generator-aws-xray": "1.1.2",
    "@opentelemetry/instrumentation-aws-sdk": "0.10.0",
    "@opentelemetry/instrumentation-dataloader": "0.3.1",
    "@opentelemetry/instrumentation-express": "0.32.4",
    "@opentelemetry/instrumentation-graphql": "0.33.2",
    "@opentelemetry/instrumentation-http": "0.34.0",
