{
  "name": "user-api",
  "main": "dist/main.js",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "watch": "tsc -w & nodemon",
    "start": "node dist/main.js",
    "dev": "npm run build && NODE_ENV=development npm run watch",
    "test": "jest \"\\.spec\\.ts\"",
    "test-integrations": "jest \"\\.integration\\.ts\" --runInBand",
    "lint": "eslint --fix-dry-run \"src/**/*.ts\"",
    "format": "eslint --fix \"src/**/*.ts\""
  },
  "dependencies": {
    "@pocket-tools/apollo-utils": "workspace:*",
    "@aws-sdk/client-eventbridge": "3.451.0",
    "@aws-sdk/client-pinpoint": "3.451.0",
    "@pocket-tools/ts-logger": "workspace:*",
    "@sentry/node": "7.80.1",
    "@sentry/tracing": "7.80.1",
    "dataloader": "2.2.2",
    "express": "4.18.2",
    "graphql": "16.8.1",
    "knex": "3.1.0",
    "lodash": "4.17.21",
    "luxon": "3.4.4",
    "mysql2": "3.6.5",
    "underscore.string": "3.3.6"
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "tsconfig": "workspace:*",
    "@faker-js/faker": "8.3.1",
    "@types/chai": "4.3.10",
    "@types/chai-datetime": "0.0.39",
    "@types/jest": "29.5.8",
    "@types/sinon": "17.0.1",
    "@types/supertest": "^2.0.16",
    "chai": "4.3.10",
    "chai-datetime": "1.8.0",
    "jest": "29.7.0",
    "jest-mock-req-res": "1.0.2",
    "nock": "13.3.8",
    "nodemon": "3.0.1",
    "sinon": "17.0.1",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1"
  }
}
