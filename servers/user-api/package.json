    "@types/node": "18.16.19",
    "@types/sinon": "10.0.15",
    "@types/supertest": "^2.0.12",
    "chai": "4.3.7",
    "chai-datetime": "1.8.0",
    "tsconfig": "workspace:*",
    "eslint-config-custom": "workspace:*",
    "jest": "26.6.3",
    "jest-mock-req-res": "1.0.2",
    "nock": "13.3.2",
    "nodemon": "3.0.1",
    "sinon": "15.2.0",
    "supertest": "6.3.3",
    "ts-jest": "26.5.6",
    "ts-node": "10.9.1"
