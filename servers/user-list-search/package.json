{
  "name": "user-list-search",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "build": "rm -rf dist && tsc && npm run build-schema",
    "build-schema": "node dist/server/buildSchema.js",
    "build:js": "npm run build",
    "dev": "NODE_ENV=development npm run build && NODE_ENV=development npm run watch",
    "format": "eslint --fix",
    "lint": "eslint --fix-dry-run",
    "prebuild": "graphql-codegen",
    "snowplow:generate": "snowtype generate",
    "start": "node dist/main.js",
    "task:es-create-index": "node dist/tasks/createESIndex.js",
    "task:queue-all-premium-users-for-backfill": "node dist/bin/loadPremiumUsersForBackfill.js",
    "test": "jest \"\\.spec\\.ts\" --runInBand ",
    "test-integrations": "jest \"\\.integration\\.ts\" --runInBand --forceExit",
    "test:watch": "npm test -- --watch --watch-extensions ts -R min --watch-files src",
    "watch": "concurrently  --names \"CodeGen,TS,Nodemon\" -c \"bgYellow,bgBlue,bgMagenta\" \"graphql-codegen --watch 'src/**/*.ts'\"  \"tsc -w --preserveWatchOutput\" \"nodemon  --config ../../nodemon.json ./dist/main.js\""
  },
  "dependencies": {
    "@apollo/server": "4.11.0",
    "@apollo/subgraph": "2.9.2",
    "@aws-sdk/client-eventbridge": "3.637.0",
    "@aws-sdk/client-sagemaker-runtime": "3.637.0",
    "@aws-sdk/client-sqs": "3.637.0",
    "@elastic/elasticsearch": "^8.14.0",
    "@opensearch-project/opensearch": "^2.10.0",
    "@pocket-tools/apollo-cursor-pagination": "1.0.3",
    "@pocket-tools/apollo-utils": "workspace:*",
    "@pocket-tools/feature-flags-client": "workspace:*",
    "@pocket-tools/sentry": "workspace:*",
    "@pocket-tools/ts-logger": "workspace:*",
    "@sentry/node": "8.30.0",
    "@smithy/node-http-handler": "3.2.0",
    "@smithy/util-retry": "3.0.3",
    "elastic-builder": "^2.29.0",
    "elasticsearch": "^16.7.3",
    "express": "4.20.0",
    "express-validator": "^7.1.0",
    "graphql": "16.9.0",
    "graphql-constraint-directive": "5.4.2",
    "graphql-tag": "2.12.6",
    "knex": "3.1.0",
    "lodash": "4.17.21",
    "mysql2": "3.11.3",
    "nanoid": "3.3.7",
    "striptags": "^3.1.1",
    "tslib": "2.7.0",
    "unleash-client": "6.1.1"
  },
  "devDependencies": {
    "@faker-js/faker": "8.4.1",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/typescript": "4.0.9",
    "@graphql-codegen/typescript-resolvers": "^4.1.0",
    "@pocket-tools/eslint-config": "workspace:*",
    "@snowplow/node-tracker": "3.24.3",
    "@snowplow/snowtype": "^0.8.2",
    "@types/elasticsearch": "^5.0.37",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.12",
    "@types/mysql": "2.15.26",
    "@types/node": "^22.5.2",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "10.0.0",
    "concurrently": "^8.2.2",
    "jest": "29.7.0",
    "jest-extended": "4.0.2",
    "nock": "14.0.0-beta.11",
    "nodemon": "3.1.4",
    "supertest": "7.0.0",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "tsconfig": "workspace:*",
    "typescript": "5.5.4"
  }
}
