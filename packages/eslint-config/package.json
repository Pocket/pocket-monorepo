{
  "name": "@pocket-tools/eslint-config",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./base.d.ts",
      "default": "./base.js"
    },
    "./cdktf": {
      "types": "./cdktf.d.ts",
      "default": "./cdktf.js"
    },
    "./servers": {
      "types": "./servers.d.ts",
      "default": "./servers.js"
    },
    "./lambda": {
      "types": "./lambda.d.ts",
      "default": "./lambda.js"
    },
    "./packages": {
      "types": "./packages.d.ts",
      "default": "./packages.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/base.js",
  "files": [
    "*.js",
    "*.d.ts",
    "package.json"
  ],
  "dependencies": {
    "@eslint/js": "^9.4.0",
    "eslint": "^9.4.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-package-json": "^0.14.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jsonc-eslint-parser": "^2.4.0",
    "prettier": "^3.3.1",
    "typescript": "5.4.5",
    "typescript-eslint": "^7.12.0"
  }
}
